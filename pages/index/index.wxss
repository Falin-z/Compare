.container{
  display: flex;
  flex-direction: column;
}
.bar{
   height: 80rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
input{
  height: 60rpx;
  flex-grow: 1;
  margin: 15rpx;
  padding: 0rpx 10rpx 0rpx 0rpx;
  border-radius:15px;
  background-color: #F0F0F0
}
.Sear{
  height: 40rpx;
  width: 40rpx;
  margin: 20rpx 10rpx 20rpx 10rpx;
}
.scanner{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  margin: 10rpx;
}
.dis{
  height: 150rpx;
  width: 170rpx;
}
.Big{
  width: 100%;
  height: 350rpx;
  margin: 10rpx 0rpx 10rpx 0rpx;
}
.notice{
  display: flex;
  flex-direction: row;

}
.laba{
  height: 40rpx;
  width: 40rpx;
  margin: 20rpx 0rpx 20rpx 10rpx;
}
button{
  font-size: 30rpx;
  margin: 10rpx;
}
button[plain] {
  border: none;
  border-color: transparent;
}
.rank{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  font-size: 27rpx;
}
.Ran{
  display: flex;
  flex-direction: column;
}
.top{
  height: 100rpx;
  width: 120rpx;
  margin-bottom: 5rpx;
}
.Box{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.box{
  height: 150rpx;
  width: 250rpx;
}